#import "UIAlertView-Helpers.h"

@implementation UIAlertView (Helpers)

+ (void) showAlertViewWithTitle:(NSString *)title message:(NSString *)message
{
    UIAlertView *alert = [[self alloc] initWithTitle:title message:message delegate:nil cancelButtonTitle:@"Close" otherButtonTitles:nil];
    [alert show];
    [alert release];
}

@end

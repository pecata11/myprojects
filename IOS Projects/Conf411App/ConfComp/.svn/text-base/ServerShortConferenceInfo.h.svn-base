#import <Foundation/Foundation.h>

@interface ServerShortConferenceInfo : NSObject {
NSString *confId;
@private
    //NSString *confId;
    NSString *name;
    NSString *date;
    NSString *venue;
    NSInteger version;
    UIImage *image;
    UIImage *bigImage;
    UIImage *mapImage;
}


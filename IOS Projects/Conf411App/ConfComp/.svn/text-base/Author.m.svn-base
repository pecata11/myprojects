#import "Author.h"

@implementation Author

@synthesize authorId = _authorId;
@synthesize firstName = _firstName;
@synthesize lastName = _lastName;
@synthesize name = _name;
@synthesize instituteId = _instituteId;
@synthesize presentations = _presentations;
@synthesize institution = _institution;
@synthesize title=_title;
@synthesize image=_image;
@synthesize institutions=_institutions;

- (void) dealloc
{
    [_firstName release];
    [_lastName release];
    [_name release];
    [_presentations release];
    [_institution release];
    [_title release];
    [_image release];
    [_institutions release];
    
    [super dealloc];
}

@end
